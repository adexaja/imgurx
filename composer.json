{
  "name": "nahid/imgurx",
  "description": "Imgurx is a package for laravel to access imgur web service based on Adyg/php-imgur-api-client",
  "keywords": ["imgur", "imgurx", "image-server", "image-host", "php-imgur-api-client"],
  "homepage": "https://github.com/nahid/imgurx",
  "type": "library",
  "license": "Creative Commons",
  "authors": [
    {
      "name": "Nahid Bin Azhar",
      "email": "nahid.dns@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.4.0",
    "adyg/php-imgur-api-client": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.1.*"
  },
  "autoload": {
    "psr-4": {
      "Nahid\\Imgurx\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
